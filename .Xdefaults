!xterm*scrollBar:	true
xterm*foreground:	#0080ff
xterm*background:	black
xterm*saveLines:	1024
xterm*loginShell:	true
xterm*cursorBlink:	true
xterm*cursorOffTime:	300
xterm*cursorOnTime:	300
xterm*alwaysHighlight:	true

xload*foreground:	blue
xload*background:	black
xload*showLabel:	false
xload*highlight:	darkred
xload*update:		3

xclock*foreground:	green
xclock*background:	black
xclock*highlight:	red
xclock*hands:		red
xclock*update:		1
xclock*padding:		0
xclock*analog:		false
xclock*render:		true
xclock*face:		DejaVu Sans Condensed-11

