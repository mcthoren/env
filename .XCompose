# partly from https://github.com/kragen/xcompose/blob/master/dotXCompose
# /usr/X11R6/share/X11/locale/en_US.UTF-8/Compose
# https://cgit.freedesktop.org/xorg/lib/libX11/plain/nls/en_US.UTF-8/Compose.pre
include "%L"
<Multi_key> <s> <u> <m>			: "∑"	U2211		# N-ARY SUMMATION
<Multi_key> <i> <n> <t>			: "∫"	U222B		# INTEGRAL
<Multi_key> <u> <i> <n> <t>		: "⨛"	U2A1B		# UPPER INTEGRAL
<Multi_key> <l> <i> <n> <t>		: "⨜"	U2A1C		# LOWER INTEGRAL
<Multi_key> <2> <n> <t>			: "∬"	U222C		# DOUBLE INTEGRAL
<Multi_key> <3> <n> <t>			: "∭"	U222D		# TRIPLE INTEGRAL
<Multi_key> <4> <n> <t>			: "⨌"	U2A0C		# QUADRUPLE INTEGRAL
<Multi_key> <o> <i> <n> <t>		: "∮"	U222E		# CONTOUR INTEGRAL
<Multi_key> <p> <i> <n> <t>		: "⨕"	U2A15		# SEMICIRCULAR POLE INTEGRAL
<Multi_key> <c> <p> <i> <n> <t>		: "⨓"	U2A13		# INTEGRAL AROUND POINT
<Multi_key> <o> <2> <n> <t>		: "∯"	U222F		# SURFACE INTEGRAL
<Multi_key> <o> <3> <n> <t>		: "∰"	U2230		# VOLUME INTEGRAL
<Multi_key> <g> <i> <n> <t>		: "⨘"	U2A18		# GEOMETRIC INTEGRAL
<Multi_key> <s> <i> <n> <t>		: "⨋"	U2A0B		# SUM/INTEGRAL
<Multi_key> <d> <e> <l>			: "∇"	U2207	        # NABLA
<Multi_key> <d> <period> <period>	: "∂"	U2202		# PARTIAL DIFFERENTIAL
<Multi_key> <h> <minus>			: "ℏ"	U210F		# PLANCK CONSTANT OVER TWO PI
<Multi_key> <h> <p>			: "ℎ"	U210E		# PLANCK CONSTANT
<Multi_key> <w> <a> <r> <n>		: "⚠"	U26A0		# WARNING SIGN
<Multi_key> <z> <a> <p>			: "⚡"	U26A1		# HIGH VOLTAGE SIGN
<Multi_key> <r> <a> <d>			: "☢"	U2622		# RADIOACTIVE SIGN
<Multi_key> <b> <i> <o>			: "☣"	U2623		# BIOHAZARD SIGN
<Multi_key> <s> <k> <l>			: "☠"	U2620		# SKULL AND CROSSBONES
<Multi_key> <f> <l> <t>			: "⏥"	U23E5		# FLATNESS
<Multi_key> <m> <l> <l>			: "×"	U00D7		# MULTIPLICATION SIGN
<Multi_key> <v> <c> <p>			: "⨯"	U2A2F		# VECTOR OR CROSS PRODUCT
<Multi_key> <e> <p>			: "ε"	U03B5		# GREEK SMALL LETTER EPSILON
<Multi_key> <r> <h> <o>			: "ρ"	U03C1		# GREEK SMALL LETTER RHO
<Multi_key> <s> <g>			: "σ"	U03C3		# GREEK SMALL LETTER SIGMA
<Multi_key> <l> <m>			: "λ"	U03BB		# GREEK SMALL LETTER LAMDA
<Multi_key> <n> <u>			: "ν"	U03BD		# GREEK SMALL LETTER NU
<Multi_key> <P> <S> <I>			: "Ψ"	U03A8		# GREEK CAPITAL LETTER PSI
<Multi_key> <p> <s> <i>			: "ψ"	U03C8		# GREEK SMALL LETTER PSI
#
# i wind up using this file as a refrence. so add some comments of things i always forget
# but already exist in X.
# <Multi_key> <numbersign> <q>		: "♩"   U2669 # QUARTER NOTE
# <Multi_key> <numbersign> <e>		: "♪"   U266a # EIGHTH NOTE
# <Multi_key> <numbersign> <E>		: "♫"   U266b # BEAMED EIGHTH NOTES
# <Multi_key> <numbersign> <S>		: "♬"   U266c # BEAMED SIXTEENTH NOTES
# <Multi_key> <numbersign> <b>		: "♭"   U266d # MUSIC FLAT SIGN
# <Multi_key> <numbersign> <f>		: "♮"   U266e # MUSIC NATURAL SIGN
# <Multi_key> <numbersign> <numbersign>	: "♯"   U266f # MUSIC SHARP SIGN
# <Multi_key> <v> <slash>		: "√"   U221a   # v / SQUARE ROOT
# <Multi_key> <slash> <v>		: "√"   U221a   # / v SQUARE ROOT
# <Multi_key> <8> <8>			: "∞"   U221e   # 8 8 INFINITY
